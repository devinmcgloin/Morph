
<!DOCTYPE html>
<html>
{{template "head"}}
   <body>
     {{ template "header" .Auth }}

      <div class="container">
        <h2>Upload</h2>

        <div class="row">

          {{template "account-sidebar"}}

          <div class="six columns">
            <form enctype="multipart/form-data" action="/api/upload" method="post">
                  <input class="u-full-width" type="file" name="uploadfile" />
                  <input type="hidden" name="token" value="{{.}}"/>
                  <input class="button-primary" type="submit" value="upload" />
            </form>
          </div>

      </div>

      </div>
   </body>
</html>
